{
  "$schema": "http://json.schemastore.org/template",
  "author": "Turgut Hakkı Özdemir",
  "classifications": [
    "Common",
    "Blazor"
  ],
  "identity": "SimIO.BlazorTemplate.CSharp",
  "name": "SimIO Blazor Application",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "shortName": "simioblazor",
  "sourceName": "BlazorTemplate",
  "sources": [
    {
      "source": "./",
      "target": "./",
      "exclude": [
        ".template.config/**"
      ]
    }
  ],
  "preferNameDirectory" : true,
  "symbols": {
    "Version": {
      "type": "generated",
      "generator": "constant",
      "replaces": "$(Version)",
      "parameters": {
        "value":"__VERSION__"
      },
      "isRequired": true
    }
  },
  "primaryOutputs": [
    {
      "path": "BlazorTemplate.csproj"
    }
  ],
  "defaultName": "SimIOBlazorApp"
}
